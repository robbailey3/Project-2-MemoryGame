{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": 2,
        "quotes": ["error", "single"],
        "require-jsdoc": 0,
        "max-len": 1, {
          "code": 80, 
          "ignoreComments": true
    },
    "env": {
        "browser": true
    }
}